{
  "name": "eslint-plugin-oro",
  "version": "0.0.0",
  "description": "Eslint rules for Oro",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "oro",
    "oroplatform",
    "orocrm",
    "orocommerce"
  ],
  "author": "Oro, Inc.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test --coverage && npm run check-coverage",
    "lint": "eslint ./lib/ index.js",
    "unit-test": "istanbul test --dir build/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --recursive --reporter dot",
    "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100 --lines 100",
    "report-coverage-html": "istanbul report --dir  build/coverage html"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "eslint": "~3.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "peerDependencies": {
    "eslint": ">=3.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/laboro/eslint-plugin-oro.git"
  },
  "homepage": "https://github.com/laboro/eslint-plugin-oro",
  "bugs": {
    "url": "https://github.com/laboro/eslint-plugin-oro/issues"
  }
}

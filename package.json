{
  "name": "eslint-plugin-oro",
  "version": "0.0.3",
  "author": "Oro, Inc (https://www.oroinc.com)",
  "license": "MIT",
  "description": "Eslint rules for Oro",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "oro",
    "oroplatform",
    "orocrm",
    "orocommerce"
  ],
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc npm run unit-test && nyc report --reporter=text-lcov > .nyc_output/lcov.info",
    "lint": "eslint ./ -c .eslintrc --ignore-path .eslintignore",
    "unit-test": "mocha tests/**/*.js --recursive --reporter=dot"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "~7.23.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "peerDependencies": {
    "eslint": ">=7.23.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/laboro/eslint-plugin-oro.git"
  },
  "homepage": "https://github.com/laboro/eslint-plugin-oro",
  "bugs": {
    "url": "https://github.com/laboro/eslint-plugin-oro/issues"
  }
}
